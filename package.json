{
  "name": "docai",
  "version": "1.0.0",
  "description": "AI-powered CLI tool for automatically generating documentation for Python, JavaScript, and TypeScript code",
  "main": "src/index.js",
  "bin": {
    "docai": "./bin/docai.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:performance": "jest tests/performance",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "node --watch src/index.js",
    "doc:watch": "nodemon --watch src --watch test --ext py,js,ts --exec \"docai generate --low-level --inline --backup --file \\\"./src/**/*.{py,js,ts}\\\" --file \\\"./test/**/*.{py,js,ts}\\\" --verbose\"",
    "doc:watch:py": "nodemon --watch . --ext py --exec \"docai generate --low-level --inline --backup --file \\\"./**/*.py\\\" --verbose\"",
    "doc:watch:js": "nodemon --watch . --ext js,ts --exec \"docai generate --low-level --inline --backup --file \\\"./**/*.{js,ts}\\\" --verbose\"",
    "doc:generate": "docai generate --low-level --inline --backup --file \"./src/**/*.{py,js,ts}\" --file \"./test/**/*.{py,js,ts}\" --verbose",
    "doc:readme": "docai generate --high-level --verbose"
  },
  "keywords": [
    "documentation",
    "ai",
    "cli",
    "python",
    "javascript",
    "typescript",
    "docstring",
    "jsdoc"
  ],
  "author": "DocAI Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "axios": "^1.12.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.2.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10"
  }
}
